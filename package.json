{
  "name": "@meeting-baas/sdk",
  "version": "6.0.0",
  "description": "Official SDK for Meeting BaaS API - https://meetingbaas.com",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm build:bundle && pnpm build:docs",
    "build:all": "pnpm build:types && pnpm build:bundle && pnpm build:docs",
    "build:types": "echo 'üì¶ Generating TypeScript types from OpenAPI specs...' && pnpm openapi:generate || echo '‚ö†Ô∏è  Type generation skipped (using existing types)'",
    "build:bundle": "echo 'üî® Building SDK bundles...' && tsup",
    "build:docs": "echo 'üìö Generating documentation...' && pnpm docs:generate",
    "build:quick": "tsup",
    "clean:build": "pnpm clean && pnpm build:bundle",
    "dev": "tsup --watch",
    "lint": "biome check",
    "lint:fix": "biome format --write",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run test/unit/",
    "test:integration": "vitest run test/integration/",
    "test:error-handling": "vitest run test/error-handling.test.ts",
    "openapi:clean": "rm -rf src/generated",
    "openapi:clean:gladia": "rm -rf src/generated/gladia",
    "openapi:clean:assemblyai": "rm -rf src/generated/assemblyai",
    "openapi:clean:deepgram": "rm -rf src/generated/deepgram",
    "openapi:generate": "pnpm openapi:clean && orval",
    "openapi:generate:gladia": "pnpm openapi:clean:gladia && orval --config orval.config.ts --project gladiaApi && orval --config orval.config.ts --project gladiaZod",
    "openapi:generate:assemblyai": "pnpm openapi:clean:assemblyai && orval --config orval.config.ts --project assemblyaiApi && orval --config orval.config.ts --project assemblyaiZod",
    "openapi:generate:deepgram": "pnpm openapi:clean:deepgram && orval --config orval.config.ts --project deepgramApi && orval --config orval.config.ts --project deepgramZod",
    "openapi:rebuild": "pnpm openapi:generate && pnpm build",
    "docs:clean": "rm -rf docs/generated",
    "docs:generate": "pnpm docs:clean && pnpm docs:generate:router && pnpm docs:generate:gladia",
    "docs:generate:router": "typedoc --options typedoc.router.config.mjs",
    "docs:generate:gladia": "typedoc --options typedoc.gladia.config.mjs",
    "docs:generate:assemblyai": "typedoc --options typedoc.assemblyai.config.mjs",
    "prepublishOnly": "pnpm lint:fix && pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meeting-baas/sdk-generator.git"
  },
  "homepage": "https://meetingbaas.com",
  "bugs": {
    "url": "https://github.com/meeting-baas/sdk-generator/issues"
  },
  "keywords": [
    "meeting",
    "baas",
    "mcp",
    "api",
    "sdk",
    "google-meet",
    "zoom",
    "microsoft-teams",
    "transcription",
    "recording",
    "meeting-bot"
  ],
  "author": "SAS SPOKE <support@meetingbaas.com> (https://meetingbaas.com)",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.0.5",
    "@faker-js/faker": "^9.8.0",
    "@types/node": "22.13.10",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "msw": "^2.3.3",
    "orval": "^7.9.0",
    "ts-node": "10.9.2",
    "tsup": "8.4.0",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "5.8.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "axios": "1.8.3",
    "zod": "3.24.2"
  },
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
}
